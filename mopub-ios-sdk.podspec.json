{
  "name": "mopub-ios-sdk",
  "module_name": "MoPub",
  "version": "4.14.0",
  "summary": "The Official MoPub Client SDK allows developers to easily monetize their apps by showing banner, interstitial, and native ads.",
  "description": "                    MoPub is a hosted ad serving solution built specifically for mobile publishers.\n                    Grow your mobile advertising business with powerful ad management, optimization \n                    and reporting capabilities, and earn revenue by connecting to the world's largest \n                    mobile ad exchange. \n\n                    To learn more or sign up for an account, go to http://www.mopub.com. \n",
  "homepage": "https://github.com/mopub/mopub-ios-sdk",
  "license": {
    "type": "New BSD",
    "file": "LICENSE"
  },
  "authors": {
    "MoPub": "support@mopub.com"
  },
  "social_media_url": "http://twitter.com/mopub",
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "git": "https://github.com/notabasement/mopub-ios-sdk.git",
    "branch": "release/4.14.0"
  },
  "frameworks": [
    "CoreGraphics",
    "CoreLocation",
    "CoreMedia",
    "CoreTelephony",
    "Foundation",
    "MediaPlayer",
    "QuartzCore",
    "SystemConfiguration",
    "UIKit"
  ],
  "weak_frameworks": [
    "AdSupport",
    "StoreKit",
    "WebKit"
  ],
  "requires_arc": true,
  "default_subspecs": "MoPubSDK",
  "subspecs": [
    {
      "name": "MoPubSDK",
      "source_files": "MoPubSDK/**/*.{h,m}",
      "resources": "MoPubSDK/**/*.{png,bundle,xib,nib}",
      "private_header_files":"MoPubSDK/Internal/Fabric/*.{h,m}"
    },
    {
      "name": "AdMob",
      "source_files": "AdNetworkSupport/GoogleAdMob/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "GoogleMobileAds": [
          ">= 7.1.0"
        ]
      },
      "xcconfig": {
        "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/GoogleMobileAds/**\""
      }
    },
    {
      "name": "AdColony",
      "source_files": "AdNetworkSupport/AdColony/*.{h,m}",
      "frameworks": [
        "WebKit"
      ],
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "SupersonicAdColonyAdapter": [

        ]
      }
    },
    {
      "name": "Chartboost",
      "source_files": "AdNetworkSupport/Chartboost/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "SupersonicChartboostAdapter": [

        ]
      }
    },
    {
      "name": "Facebook",
      "source_files": "AdNetworkSupport/Facebook/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "FBAudienceNetwork": [

        ]
      }
    },
    {
      "name": "Greystripe",
      "source_files": "AdNetworkSupport/Greystripe/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "GreystripeSDK": [

        ]
      }
    },
    {
      "name": "InMobi",
      "source_files": "AdNetworkSupport/InMobi/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "InMobiSDK": [
          ">= 5.0.0"
        ]
      }
    },
    {
      "name": "Millennial",
      "source_files": "Forks/nimrich-Millennial/AdNetworkSupport/Millennial/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "MMAdSDK": [
          ">= 6.3.0"
        ]
      }
    },
    {
      "name": "Vungle",
      "source_files": "AdNetworkSupport/Vungle/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "SupersonicVungleAdapter": [

        ]
      }
    },
    {
      "name": "TapJoy",
      "source_files": "AdNetworkSupport/TapJoy/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "TapjoySDK": [

        ]
      }
    },
    {
      "name": "Unity",
      "source_files": "AdNetworkSupport/Unity/*.{h,m}",
      "dependencies": {
        "mopub-ios-sdk/MoPubSDK": [

        ],
        "SupersonicUnityAdsAdapter": [

        ]
      }
    }
  ]
}